// Simple test program in the edu language

// Variable declaration
int x = 42;
float y = 3.14;
string message = "Hello, World!";

// Function declaration
int function add(int a, int b) {
  return a + b;
}

// Class declaration
class Calculator {
  constructor() {
    print("Calculator created");
  }

  int function multiply(int a, int b) {
    return a * b;
  }
}

// Main function
void function main() {
  print(message);
  
  int result = add(x, 10);
  print(result);
  
  Calculator calc = Calculator();
  int product = calc.multiply(5, 7);
  print(product);
}